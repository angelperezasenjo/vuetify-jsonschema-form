{
  "name": "@amplia/vjsf",
  "version": "3.0.0",
  "description": "Generate forms for the vuetify UI library (vuejs) based on annotated JSON schemas.",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint --ext js .",
    "lint-fix": "eslint --fix --ext js .",
    "build": "rm -rf dist && webpack && cp lib/VJsf.css dist/main.css",
    "prepare": "npm run lint && npm test && npm run build && npm run doc-build",
    "postpublish": "gh-pages-multi deploy -v -s doc/dist -t latest",
    "test": "jest",
    "clear-cache": "jest tests/unit --clearCache",
    "test-watch": "jest --watch",
    "test-update": "jest --updateSnapshot",
    "doc-build": "(cd doc && TARGET=https://amplia-iiot/vuetify-jsonschema-form/develop/ nuxt generate)",
    "doc-master": "(cd doc && TARGET=https://amplia-iiot/vuetify-jsonschema-form/master/ nuxt generate) && gh-pages-multi deploy -v -s doc/dist -t master",
    "analyze": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|mp3)$": "jest-transform-stub",
      "^.+\\.m?[j|t]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.{js,vue}"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vuetify)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scarfpurple/vuetify-jsonschema-form.git"
  },
  "author": "Angel Perez <scarfpurple97@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koumoul-dev/vuetify-jsonschema-form/issues"
  },
  "homepage": "https://github.com/koumoul-dev/vuetify-jsonschema-form#readme",
  "dependencies": {
    "babel-core": "^7.0.0-0",
    "debounce": "^1.2.1",
    "debounce-promise": "^3.1.2",
    "debug": "^4.3.3",
    "fast-copy": "^2.1.1",
    "fast-equals": "^2.0.4",
    "match-all": "^1.2.6",
    "vue": "^3.2.36",
    "vuetify": "^3.4.3"
  },
  "optionalDependencies": {
    "@mdi/font": "^6.5.95",
    "@mdi/js": "^6.5.95",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "expr-eval": "^2.0.2",
    "markdown-it": "^12.3.2",
    "property-expr": "^2.0.5",
    "vuedraggable": "^2.24.3"
  },
  "peerDependencies": {
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@koumoul/data-fair-search-widget": "^0.3.0",
    "@koumoul/gh-pages-multi": "^0.6.0",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "27",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.8.1",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "file-loader": "^6.2.0",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "random-words": "^1.1.2",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "v-mask": "^2.3.0",
    "vue-axios": "^2.1.5",
    "vue-loader": "^17.3.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
